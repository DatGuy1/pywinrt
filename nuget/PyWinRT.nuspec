<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata minClientVersion="2.5">
    <id>PyWinRT</id>
    <version>$version$</version>
    <title>Py/WinRT tool</title>
    <authors>Microsoft,dlech</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Py/WinRT is a tool for generating a Python projection of any Windows Runtime library.</description>
    <readme>docs\README.md</readme>
    <releaseNotes>See https://github.com/pywinrt/pywinrt/blob/main/CHANGELOG.md</releaseNotes>
    <tags>Python, WinRT</tags>
    <copyright>© Microsoft Corporation. All rights reserved.</copyright>
    <license type="file">LICENSE</license>
    <projectUrl>https://github.com/pywinrt/pywinrt</projectUrl>
    <repository type="git" url="https://github.com/pywinrt/pywinrt.git" />
    <icon>images\icon.png</icon>
  </metadata>
  <files>
    <file src="..\LICENSE"/>
    <file src="$pywinrt_exe$" target="bin\"/>
    <file src="README.md" target="docs\" />
    <file src="icon.png" target="images\" />
  </files>
</package>
